				<apply icon="piste_type_sled"/>
			</switch>
			<apply>
				<case additional="piste:difficulty=novice" icon_2="skimap_overlay_novice"/>
				<case additional="piste:difficulty=easy" icon_2="skimap_overlay_easy"/>
				<case additional="piste:difficulty=intermediate" icon_2="skimap_overlay_intermediate"/>
				<case additional="piste:difficulty=advanced" icon_2="skimap_overlay_advanced"/>
				<case additional="piste:difficulty=expert" icon_2="skimap_overlay_expert"/>
				<case additional="piste:difficulty=freeride" icon_2="skimap_overlay_freeride"/>
				<case additional="piste:difficulty=extreme" icon_2="skimap_overlay_extreme"/>
			</apply>
			<apply icon__1="skimap_white_black_round_shield">
				<apply_if nightMode="true" icon__1="skimap_gray_black_round_shield"/>
			</apply>
		</switch>-->
		<case minzoom="15" tag="amenity" value="ski_rental" icon="amenity_ski_rental" shield="white_blue_square_shield">
			<apply_if nightMode="true" shield="gray_blue_square_shield"/>
		</case>
	</point>


	<polygon>
		<!--Winter landscape-->
		<switch>
			<case minzoom="12" tag="natural" value="field"/>
			<apply_if minzoom="14" color_2="#f5ffff" strokeWidth_2=":1"/>
			<apply_if nightMode="true" color="#004333" color_2="#004333"/>
		</switch>
		<switch>
			<case minzoom="11" tag="natural" value="beach" shader="beach"/>
			<case minzoom="12" tag="natural" value="mud" shader="mud"/>
			<apply_if nightMode="true" shader="" color="#334433"/>
		</switch>
		<case minzoom="12" tag="natural" value="scrub" color="#8decb6" shader="">
			<apply_if nightMode="true" shader="" color="#002014"/>
		</case>

		<switch minzoom="6">
			<case tag="natural" value="wood"/>
			<case tag="landuse" value="forest"/>
			<apply color="$woodColor" shader=""/>
		</switch>
		<switch>
			<case tag="place" value="island"/>
			<case tag="natural" value="land"/>
			<case tag="natural" value="grassland"/>
			<apply color="#f5ffff">
				<apply_if nightMode="true" color="#002a2a"/>
			</apply>
		</switch>

		<switch minzoom="12">
			<case tag="piste:type" value="downhill"/>
			<case tag="piste:type" value="skitour"/>
			<case tag="piste:type" value="hike"/>
			<case tag="piste:type" value="sled"/>
			<case tag="piste:type" value="sleigh"/>
			<case tag="piste:type" value="snow_park">
				<apply color_2="$pisteDefaultColor">
					<case maxzoom="12" pathEffect_2="16_2_3_2" strokeWidth_2="1.5:1.5"/>
					<case maxzoom="14" pathEffect_2="22_4_5_4" strokeWidth_2="2.5:2.5"/>
					<case maxzoom="16" pathEffect_2="27_6_9_6" strokeWidth_2="3.5:3.5"/>
					<case minzoom="17" pathEffect_2="40_7_10_7" strokeWidth_2="4.5:4.5"/>
				</apply>
			</case>
			<apply color="$pisteAreaDefaultColor">
				<case additional="piste:difficulty=novice" color="$pisteAreaDifficultyNoviceColor" color_2="$pisteDifficultyNoviceColor"/>
				<case additional="piste:difficulty=easy" color="$pisteAreaDifficultyEasyColor" color_2="$pisteDifficultyEasyColor"/>
				<case additional="piste:difficulty=intermediate" color="$pisteAreaDifficultyIntermediateColor" color_2="$pisteDifficultyIntermediateColor"/>
				<case additional="piste:difficulty=advanced" color="$pisteAreaDifficultyAdvancedColor" color_2="$pisteDifficultyAdvancedColor"/>
				<case additional="piste:difficulty=expert" color="$pisteAreaDifficultyExpertColor" color_2="$pisteDifficultyExpertColor"/>
				<case additional="piste:difficulty=freeride" color="$pisteAreaDifficultyFreerideColor" color_2="$pisteDifficultyFreerideColor"/>
				<case additional="piste:difficulty=extreme" color="$pisteAreaDifficultyExtremeColor" color_2="$pisteDifficultyExtremeColor"/>
			</apply>
		</switch>

		<case minzoom="11" tag="leisure" value="ski_resort" color="$null" strokeWidth_2="1:1" color_2="#99464fff" iconOrder="10">
			<apply_if nightMode="true" color_2="#99d0d2ff"/>
		</case>

	</polygon>


	<line>

		<switch minzoom="11">
			<switch>
				<case tag="piste:type" value="downhill"/>
				<case tag="piste:type" value="skitour">
					<apply>
						<case maxzoom="11" pathEffect="4_4"/>
						<case maxzoom="14" pathEffect="7_7"/>
						<case maxzoom="16" pathEffect="10_10"/>
						<case minzoom="17" pathEffect="13_13"/>
					</apply>
				</case>
				<apply>
					<case maxzoom="11" strokeWidth="2:2"/>
					<case maxzoom="13" strokeWidth="3:3"/>
					<case maxzoom="14" strokeWidth="4:4"/>
					<case maxzoom="15" strokeWidth="5:5"/>
					<case maxzoom="16" strokeWidth="7:7"/>
					<case minzoom="17" strokeWidth="10:10"/>
				</apply>
				<apply_if pisteRoutes="true" additional="route=piste">
					<case maxzoom="11" strokeWidth__1="2:2" strokeWidth__2="4:4"/>
					<case maxzoom="13" strokeWidth__1="3:3" strokeWidth__2="6:6"/>
					<case maxzoom="14" strokeWidth__1="4:4" strokeWidth__2="8:8"/>
					<case maxzoom="15" strokeWidth__1="5:5" strokeWidth__2="9:9"/>
					<case maxzoom="16" strokeWidth__1="7:7" strokeWidth__2="13:13"/>
					<case minzoom="17" strokeWidth__1="10:10" strokeWidth__2="16:16"/>
				</apply_if>
			</switch>
			<switch>
				<switch>
					<case tag="piste:type" value="nordic"/>
					<case tag="piste:type" value="hike">
						<apply>
							<case maxzoom="12" pathEffect="4_1"/>
							<case maxzoom="14" pathEffect="6_3"/>
							<case maxzoom="16" pathEffect="9_4"/>
							<case minzoom="17" pathEffect="12_5"/>
						</apply>
					</case>
					<apply>
						<case maxzoom="13" strokeWidth="1:1"/>
						<case maxzoom="14" strokeWidth="2.5:2.5"/>
						<case maxzoom="16" strokeWidth="3.5:3.5"/>
						<case minzoom="17" strokeWidth="4.5:4.5"/>
					</apply>
				</switch>
				<switch>
					<switch>
						<case tag="piste:type" value="sled"/>
						<case tag="piste:type" value="sleigh"/>
						<apply>
							<case maxzoom="12" pathEffect="14_1"/>
							<case maxzoom="14" pathEffect="19_2"/>
							<case maxzoom="16" pathEffect="22_3"/>
							<case minzoom="17" pathEffect="30_4"/>
						</apply>
					</switch>
					<case tag="piste:type" value="snow_park">
						<apply>
							<case maxzoom="12" pathEffect="16_2_3_2"/>
							<case maxzoom="14" pathEffect="22_4_5_4"/>
							<case maxzoom="16" pathEffect="27_6_9_6"/>
							<case minzoom="17" pathEffect="40_7_10_7"/>
						</apply>
					</case>
					<apply>
						<case maxzoom="12" strokeWidth="1:1"/>
						<case maxzoom="14" strokeWidth="2.5:2.5"/>
						<case maxzoom="16" strokeWidth="3.5:3.5"/>
						<case minzoom="17" strokeWidth="4.5:4.5"/>
					</apply>
				</switch>
				<apply_if pisteRoutes="true" additional="route=piste">
					<case maxzoom="11" strokeWidth__1="1:1" strokeWidth__2="2:2"/>
					<case maxzoom="12" strokeWidth__1="1:1" strokeWidth__2="2.5:2.5"/>
					<case maxzoom="13" strokeWidth__1="1:1" strokeWidth__2="2.7:2.7"/>
					<case maxzoom="14" strokeWidth__1="2.5:2.5" strokeWidth__2="5:5"/>
					<case maxzoom="16" strokeWidth__1="3.5:3.5" strokeWidth__2="6.5:6.5"/>
					<case minzoom="17" strokeWidth__1="4.5:4.5" strokeWidth__2="9:9"/>
				</apply_if>
			</switch>
			<case tag="piste:type" value="snow_park"/>
			<apply>
				<case additional="piste:oneway=yes">
					<case minzoom="14" maxzoom="14" pathIcon="arrow_triangle_black_small_nobg" pathIconStep="35">
						<apply_if nightMode="true" pathIcon="arrow_triangle_white_small_nobg"/>
					</case>
					<case minzoom="15" maxzoom="15" pathIcon="arrow_triangle_black_nobg" pathIconStep="40">
						<apply_if nightMode="true" pathIcon="arrow_triangle_white_nobg"/>
					</case>
					<case minzoom="16" pathIcon="arrow_triangle_black_big_nobg" pathIconStep="50">
						<apply_if nightMode="true" pathIcon="arrow_triangle_white_big_nobg"/>
					</case>
				</case>
				<case additional="piste:oneway=no">
					<case minzoom="14" maxzoom="14" pathIcon="skimap_arrow_2triangles_black_small_nobg" pathIconStep="45">
						<apply_if nightMode="true" pathIcon="skimap_arrow_2triangles_white_small_nobg"/>
					</case>
					<case minzoom="15" maxzoom="15" pathIcon="skimap_arrow_2triangles_black_nobg" pathIconStep="60">
						<apply_if nightMode="true" pathIcon="skimap_arrow_2triangles_white_nobg"/>
					</case>
					<case minzoom="16" pathIcon="skimap_arrow_2triangles_black_big_nobg" pathIconStep="80">
						<apply_if nightMode="true" pathIcon="skimap_arrow_2triangles_white_big_nobg"/>
					</case>
				</case>
			</apply>
			<apply_if additional="piste:abandoned=yes" cap="ROUND">
				<case maxzoom="12" pathEffect="1_17"/>
				<case maxzoom="14" pathEffect="1_30"/>
				<case maxzoom="16" pathEffect="1_45"/>
				<case minzoom="17" pathEffect="1_65"/>
			</apply_if>
			<apply color="$pisteDefaultColor" color__2="$pisteRouteColor"/>
			<apply color__1="#ffffff"> <!-- Piste route substrate -->
				<apply_if nightMode="true" color__1="#000000"/>
			</apply>
			<apply>
				<switch>
					<case additional="piste:difficulty=novice" color="$pisteDifficultyNoviceColor" color_2="$pisteGroomingNoviceColor" color_3="$pisteGroomingNoviceColor"/>
					<case additional="piste:difficulty=easy" color="$pisteDifficultyEasyColor" color_2="$pisteGroomingEasyColor" color_3="$pisteGroomingEasyColor"/>
					<case additional="piste:difficulty=intermediate" color="$pisteDifficultyIntermediateColor" color_2="$pisteGroomingIntermediateColor" color_3="$pisteGroomingIntermediateColor"/>
					<case additional="piste:difficulty=advanced" color="$pisteDifficultyAdvancedColor" color_2="$pisteGroomingAdvancedColor" color_3="$pisteGroomingAdvancedColor"/>
					<case additional="piste:difficulty=expert" color="$pisteDifficultyExpertColor" color_2="$pisteGroomingExpertColor" color_3="$pisteGroomingExpertColor"/>
					<case additional="piste:difficulty=freeride" color="$pisteDifficultyFreerideColor" color_2="$pisteGroomingFreerideColor" color_3="$pisteGroomingFreerideColor"/>
					<case additional="piste:difficulty=extreme" color="$pisteDifficultyExtremeColor" color_2="$pisteGroomingExtremeColor" color_3="$pisteGroomingExtremeColor"/>
				</switch>
			</apply>
			<apply minzoom="14" pisteGrooming="true">
				<switch>
					<case additional="piste:grooming=classic"/>
					<case additional="piste:grooming=mogul"/>
					<switch>
						<case additional="piste:grooming=skating" pathEffect_2="32_4" pathEffect_3="2_2_2_30" CAP_3="BUTT"/>
						<case additional="piste:grooming=classic+skating" pathEffect_3="2_30" CAP_3="BUTT"/>
						<apply>
							<case maxzoom="14" strokeWidth_3="3:3"/>
							<case minzoom="15" strokeWidth_3="4:4"/>
						</apply>
					</switch>
					<case additional="piste:grooming=scooter" pathEffect_2="17_4"/>
					<case additional="piste:grooming=backcountry" pathEffect_2="5_20"/>
					<apply>
						<case maxzoom="14" strokeWidth_2="1:1"/>
						<case minzoom="15" strokeWidth_2="2:2"/>
					</apply>
				</switch>
			</apply>
		</switch>

		<switch minzoom="12">
			<switch>
				<case tag="aerialway" value="gondola"/>
				<case tag="aerialway" value="cable_car"/>
				<apply>
					<case maxzoom="14" strokeWidth="1:1"/>
					<case maxzoom="16" strokeWidth="2:2"/>
					<case minzoom="17" strokeWidth="3:3"/>
				</apply>
			</switch>
			<switch>
				<switch>
					<case tag="aerialway" value="chair_lift"/>
					<case tag="aerialway" value="t-bar"/>
					<case tag="aerialway" value="j-bar"/>
					<case tag="aerialway" value="platter"/>
					<case tag="aerialway" value="magic_carpet"/>
					<case tag="aerialway" value="rope_tow"/>
					<apply minzoom="14" pathIcon="arrow_triangle_white2">
						<apply_if nightMode="true" pathIcon="arrow_triangle_black2"/>
					</apply>
				</switch>
				<case tag="aerialway" value="mixed_lift"/>
				<apply>
					<case maxzoom="14" strokeWidth="0.6"/>
					<case maxzoom="16" strokeWidth="1:1"/>
					<case minzoom="17" strokeWidth="1.5:1.5"/>
				</apply>
			</switch>
			<case tag="aerialway" value="drag_lift">
				<case maxzoom="14" strokeWidth="0.5"/>
				<case maxzoom="16" strokeWidth="0.8"/>
				<case minzoom="17" strokeWidth="1:1"/>
			</case>
			<apply>
				<switch minzoom="14">
					<case additional="oneway=no" pathIcon="arrow_2triangles_white">
						<apply_if nightMode="true" pathIcon="arrow_2triangles_black"/>
					</case>
					<case additional="oneway=yes" pathIcon="arrow_triangle_white2">
						<apply_if nightMode="true" pathIcon="arrow_triangle_black2"/>
					</case>
				</switch>
			</apply>
			<apply>
				<case maxzoom="15" pathIconStep="30"/>
				<case maxzoom="16" pathIconStep="35"/>
				<case minzoom="17" pathIconStep="45"/>
			</apply>
			<apply color="#000000">
				<apply_if nightMode="true" color="#ffffff"/>
			</apply>
		</switch>
		<switch engine_v1="false" minzoom="14" strokeWidth="0.3">
			 <case tag="natural" value="cliff" color="#148c14" pathIcon="stroke_darkgreen" pathIconStep="10"/>
			 <case tag="natural" value="slope" color="#88949494" pathIcon="stroke_gray" pathIconStep="10"/>
		</switch>


	</line>
</renderingStyle>