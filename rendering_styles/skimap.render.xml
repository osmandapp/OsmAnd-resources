	<renderingConstant name="null" value="#00000000"/>


	<order>
		<switch area="false" point="true" objectType="1" order="128">
			<case tag="piste:type" value="downhill"/>
			<case tag="piste:type" value="nordic"/>
			<case tag="piste:type" value="skitour"/>
			<case tag="piste:type" value="hike"/>
			<case tag="piste:type" value="sled"/>
			<case tag="piste:type" value="sleigh"/>
			<case tag="leisure" value="ski_resort"/>
		</switch>
		<switch cycle="true" point="false" objectType="3" order="5">
			<case tag="piste:type" value="downhill"/>
			<case tag="piste:type" value="skitour"/>
			<case tag="piste:type" value="hike"/>
			<case tag="piste:type" value="sled"/>
			<case tag="piste:type" value="sleigh"/>
			<case tag="piste:type" value="snow_park"/>
			<case tag="leisure" value="ski_resort"/>
		</switch>

		<switch objectType="2" order="80">
			<case tag="piste:type" value="nordic" order="100"/>
			<case tag="piste:type" value="downhill" order="100"/>
			<case tag="piste:type" value="skitour" order="100"/>
			<case tag="piste:type" value="hike" order="100"/>
			<case tag="piste:type" value="sled" order="100"/>
			<case tag="piste:type" value="sleigh" order="100"/>
			<case tag="piste:type" value="snow_park" order="100"/>
			<case tag="aerialway" value="" order="101"/>
		</switch>
	</order>


	<text>
		<switch minzoom="14" textOrder="8">
			<case tag="piste:type" value="nordic" nameTag="piste:name"/>
			<case tag="piste:type" value="nordic" nameTag=""/>
			<case tag="piste:type" value="downhill" nameTag="piste:name"/>
			<case tag="piste:type" value="downhill" nameTag=""/>
			<case tag="piste:type" value="skitour" nameTag="piste:name"/>
			<case tag="piste:type" value="skitour" nameTag=""/>
			<case tag="piste:type" value="hike" nameTag="piste:name"/>
			<case tag="piste:type" value="hike" nameTag=""/>
			<case tag="piste:type" value="sled" nameTag="piste:name"/>
			<case tag="piste:type" value="sled" nameTag=""/>
			<case tag="piste:type" value="sleigh" nameTag="piste:name"/>
			<case tag="piste:type" value="sleigh" nameTag=""/>
			<case tag="piste:type" value="snow_park"/>
			<apply>
				<case maxzoom="14" textSize="12"/>
				<case maxzoom="15" textSize="13"/>
				<case maxzoom="16" textSize="14"/>
				<case maxzoom="17" textSize="16"/>
				<case minzoom="18" textSize="18"/>
			</apply>
			<apply textColor="$pisteTextColor" textBold="true" textHaloRadius="3" textHaloColor="$pisteDefaultColor" textOnPath="true" textWrapWidth="25"/>
<!--			<apply_if engine_v1="true" textDy="4"/>-->
			<apply>
				<case additional="piste:difficulty=intermediate" textHaloColor="$pisteDifficultyIntermediateTextHaloColor"/>
				<case additional="piste:difficulty=easy" textHaloColor="$pisteDifficultyEasyTextHaloColor"/>
				<case additional="piste:difficulty=advanced" textHaloColor="$pisteDifficultyAdvancedColor"/>
				<case additional="piste:difficulty=novice" textHaloColor="$pisteDifficultyNoviceTextHaloColor"/>
				<switch>
					<case additional="piste:difficulty=expert" textHaloColor="$pisteDifficultyExpertTextHaloColor"/>
					<case additional="piste:difficulty=freeride" textHaloColor="$pisteDifficultyFreerideTextHaloColor" textColor="#333333"/>
					<apply_if nightMode="true" textColor="#222222"/>
				</switch>
				<case additional="piste:difficulty=extreme" textHaloColor="$pisteDifficultyExtremeColor"/>
			</apply>
			<apply_if pisteRoutes="true" additional="route=piste" textColor="#ffffff" textHaloColor="$pisteRouteColor" nameTag2="ref"/>
		</switch>

		<case minzoom="11" tag="leisure" value="ski_resort" textColor="#080e94" textBold="true" textHaloRadius="4" textHaloColor="#aab9bcff" textWrapWidth="25" textOrder="2">
			<apply>
				<case maxzoom="16" textSize="14"/>
				<case maxzoom="17" textSize="16"/>
				<case minzoom="18" textSize="18"/>
			</apply>
			<apply_if nightMode="true" textColor="#eff0ff" textHaloColor="#44656cf6"/>
		</case>
		<switch textSize="12" textColor="#0066ff" textHaloRadius="1" textHaloColor="#f5ffff">
			<case tag="amenity" value="ski_rental"/>
			<apply_if nightMode="true" textHaloRadius="2" textColor="#b9d5ff" textHaloColor="#88355777"/>
			<apply_if engine_v1="true" textDy="9"/>
		</switch>

		<switch minzoom="14" textColor="#ffffff" textHaloRadius="2" textBold="true" textHaloColor="#99000000" textOrder="10">
			<case tag="aerialway" value="cable_car"/>
			<case tag="aerialway" value="gondola"/>
			<case tag="aerialway" value="chair_lift"/>
			<case tag="aerialway" value="mixed_lift"/>
			<case tag="aerialway" value="drag_lift"/>
			<case tag="aerialway" value="t-bar"/>
			<case tag="aerialway" value="j-bar"/>
			<case tag="aerialway" value="platter"/>
			<case tag="aerialway" value="magic_carpet"/>
			<case tag="aerialway" value="rope_tow"/>
			<apply nameTag2="ref" textOnPath="true">
				<case maxzoom="14" disable="true"/>
				<case maxzoom="15" disable="true"/>
				<case maxzoom="16" textSize="14"/>
				<case maxzoom="17" textSize="16"/>
				<case minzoom="18" textSize="18"/>
				<apply_if nightMode="true" textColor="#dddddd" textHaloColor="#000000"/>
			</apply>
		</switch>

	</text>


