			<apply_if nightMode="true" attrColorValue="#003333"/>
		</case>
	</renderingAttribute>
	<renderingProperty attr="pisteGrooming" name="Piste grooming" description="Indicates the style of piste preparation" type="boolean" possibleValues="" category="details"/>
	<renderingProperty attr="pisteRoutes" name="Piste routes" description="Indicates piste routes" type="boolean" possibleValues="" category="routes"/>


