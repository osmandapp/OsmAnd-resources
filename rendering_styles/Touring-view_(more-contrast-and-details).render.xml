	<renderingConstant name="shadowColorLowZoom" value="#262626"/>

	<renderingConstant name="constructionRoadColor" value="#ff6600"/>
	<renderingConstant name="constructionRoadColor2" value="#ffffff"/>
	<renderingConstant name="proposedRoadColor" value="#ff6600"/>
	<renderingConstant name="proposedRoadColor2" value="#9f9f9f"/>
	<renderingConstant name="racewayColorDay" value="#ffc5cf"/>
	<renderingConstant name="racewayColorNight" value="#cd9fa7"/>

	<renderingConstant name="accessPrivateColor" value="#feb3b3"/>

	<renderingConstant name="contourLineColor" value="#2Aff7c00"/>
	<renderingConstant name="contourLineColor50m" value="#2Aea7300"/>

	<renderingConstant name="sacScaleColorT1" value="#000000"/>
	<renderingConstant name="sacScaleColorT2" value="#ff0000"/>
	<renderingConstant name="sacScaleColorT3" value="#ff0000"/>
	<renderingConstant name="sacScaleColorT4" value="#0033ff"/>
	<renderingConstant name="sacScaleColorT5" value="#0033ff"/>
	<renderingConstant name="sacScaleColorT6" value="#0033ff"/>

	<renderingConstant name="cycleRoutelcnColor" value="#400000ff"/>
	<renderingConstant name="cycleRoutercnColor" value="#40ff00ff"/>
	<renderingConstant name="cycleRoutencnColor" value="#40ff4000"/>
	<renderingConstant name="cycleRouteicnColor" value="#40ff0000"/>

	<renderingConstant name="null" value="#00000000"/>

	<!-- Area colors -->
	<renderingConstant name="defaultColor" value="#f1eae4"/>
	<renderingConstant name="defaultColorNight" value="#002a2a"/>
	<renderingConstant name="openNatureColorNight" value="#004333"/>
	<renderingConstant name="shaderColorNight" value="#334433"/>

	<renderingConstant name="forestColorDay" value="#9dd57c"/>
	<renderingConstant name="forestColorNight" value="#001122"/>
	<renderingConstant name="woodColorDay" value="#aed1a0"/>
	<renderingConstant name="woodColorNight" value="#001122"/>

	<renderingConstant name="waterColorDay" value="#b5d0d0"/>
	<renderingConstant name="waterColorNight" value="#330099"/>

	<renderingConstant name="buildingColorDay" value="#bca9a9"/>
	<renderingConstant name="buildingColorNight" value="#685454"/>

	<renderingConstant name="landuseConstructionColorDay" value="#ffffff"/>
	<renderingConstant name="landuseConstructionColorDayStroke" value="#ff6600"/>
	<renderingConstant name="landuseConstructionColorNight" value="#ffffff"/>
	<renderingConstant name="landuseConstructionColorNightStroke" value="#ff6600"/>

	<!-- Text colors -->
	<!-- no modification for now -->


	<order>
<!--		<filter appMode="bicycle" showCycleRoutes="true" tag="lcn_network" value="lcn" order="110"/>
		<filter appMode="bicycle" showCycleRoutes="true" tag="rcn_network" value="rcn" order="111"/>
		<filter appMode="bicycle" showCycleRoutes="true" tag="ncn_network" value="ncn" order="112"/>
		<filter appMode="bicycle" showCycleRoutes="true" tag="icn_network" value="icn" order="113"/>
-->
	</order>


	<text>
<!--		<group>
			<filter appMode="bicycle" showCycleRoutes="true" tag="lcn_network" value="lcn" nameTag="lcn_ref" textColor="$cyclewayColorDay" textOrder="22"/>
			<filter appMode="bicycle" showCycleRoutes="true" tag="rcn_network" value="rcn" nameTag="rcn_ref" textColor="$cyclewayColorDay" textOrder="21"/>
			<filter appMode="bicycle" showCycleRoutes="true" tag="ncn_network" value="ncn" nameTag="ncn_ref" textColor="$cyclewayColorDay" textOrder="20"/>
			<filter appMode="bicycle" showCycleRoutes="true" tag="icn_network" value="icn" nameTag="icn_ref" textColor="$cyclewayColorDay" textOrder="19"/>
			<groupFilter appMode="bicycle" textOnPath="true" >
				<filter minzoom="13" maxzoom="13" textDy="-11" textHaloRadius="1" textSize="10"/>
				<filter minzoom="14" maxzoom="14" textDy="-12" textHaloRadius="1" textSize="11"/>
				<filter minzoom="15" maxzoom="15" textDy="-14" textHaloRadius="1" textSize="13"/>
				<filter minzoom="16" maxzoom="16" textDy="-16" textHaloRadius="1" textSize="15"/>
				<filter minzoom="17" maxzoom="17" textDy="-20" textHaloRadius="1" textSize="18"/>
				<filter minzoom="18" textDy="-25" textHaloRadius="1" textSize="21"/>
			</groupFilter>
		</group>
-->
