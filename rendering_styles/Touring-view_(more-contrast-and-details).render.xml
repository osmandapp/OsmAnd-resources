				<filter appMode="car" tag="highway" value="motorway_link" nightMode="true" color="$motorwayRoadColorDimmed" color_0="$motorwayRoadColor0Dimmed"/>
				<filter appMode="car" tag="highway" value="trunk_link" nightMode="true" color="$trunkRoadColorDimmed" color_0="$trunkRoadColor0Dimmed"/>
				<filter tag="highway" value="trunk_link" nightMode="true" color="$trunkRoadColorNight" color_0="$trunkRoadColor0Night"/>
